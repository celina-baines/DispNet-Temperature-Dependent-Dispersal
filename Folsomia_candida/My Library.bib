@article{holopainenAphidResponseElevated2002,
  title = {Aphid Response to Elevated Ozone and {{CO2}}},
  author = {Holopainen, Jarmo K.},
  year = 2002,
  journal = {Entomologia Experimentalis et Applicata},
  volume = {104},
  number = {1},
