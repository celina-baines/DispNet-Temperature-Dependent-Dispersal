@article{norgaardCanPathogensOptimize2019,
  title = {Can Pathogens Optimize Both Transmission and Dispersal by Exploiting Sexual Dimorphism in Their Hosts?},
  author = {N{\o}rgaard, Louise Solveig and Phillips, Ben L. and Hall, Matthew D.},
  year = 2019,
  month = jun,
  journal = {Biology Letters},
